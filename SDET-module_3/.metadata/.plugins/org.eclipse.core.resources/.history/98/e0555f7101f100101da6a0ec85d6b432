/**
 * 
 */
package com.capgemini.string.stringbuilder.stringbuffer.immutable.mutable;

/**
 * 
 */
public interface JavaApplication {
	public static void stringExecution() {
		String s1="ja";
		System.out.println(s1);
		String s2="va";
		System.out.println(s2);
		String s3=s1+s2;
		System.out.println(s3);
		System.out.println(s1==s2);
		System.out.println(s2==s3);
		System.out.println(s1==s3);
		System.out.println();
		System.out.println();
	}
	
	public static void stringBuilderExecution() {
		StringBuilder s1 = new StringBuilder("Ra");
		System.out.println(s1);
		StringBuilder s2 = new StringBuilder("ni");
		System.out.println(s1);
		
		StringBuilder s3= s1.append(s2);
		System.out.println(s3);
		
		
		
	}

}
