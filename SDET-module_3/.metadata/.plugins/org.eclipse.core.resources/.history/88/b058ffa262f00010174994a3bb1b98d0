package com.practice.questions.Display.digit.from.one.to.nine;

import java.lang.reflect.Array;

/**
 * 
 */
public interface DisplayDigits {
	
	public static void displayDigit() {
		Integer[] arr = (Integer[]) Array.newInstance(Integer.class, 10);
		for(int i=0;i<arr.length;i++) {
			Array.set(arr, i, i);
		}
		System.out.println();
		System.out.println("Ditis: ");
		for (int i = 0; i < arr.length; i++) {
			System.out.println(Array.get(arr, i));
			
		}
	}
	
	

}
