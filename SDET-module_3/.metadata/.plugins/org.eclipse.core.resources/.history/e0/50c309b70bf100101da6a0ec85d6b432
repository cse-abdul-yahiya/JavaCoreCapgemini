/**
 * 
 */
package com.capgemini.inbuiltmethodofstring;

/**
 * 
 */
public interface JavaApplication {
public static void execution1() {
	String s1= "09AZaz";
	System.out.println("Length of "+s1+" is "+s1.length());
	System.out.println();
	System.out.println("Each Character of "+s1+" and their ascii Value is: ");
	for (int i = 0; i < s1.length(); i++) {
		System.out.println(s1.charAt(i)+" "+s1.codePointAt(i));
	}
	System.out.println();
	System.out.println();
}

public static void execution2() {
	String s1="core";
	System.out.println(s1);
	String s2 = s1.toUpperCase();
	System.out.println(s2);
	System.out.println(s1==s2);
	String s3 ="JAVA";
	System.out.println(s3);
	String s4 = s3.toLowerCase();
	System.out.println(s4);
	System.out.println(s3==s4);
	System.out.println();
	System.out.println();
	
}



}
